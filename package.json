{
  "name": "Chillax",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "testin": "echo \"Error: no test specified\" && exit 1",
    "client": "npm run dev --prefix client",
    "server": "nodemon index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "testOwn": "echo 'No Tests'",
    "test": "mocha ./test/*.test.js --timeout 10000",
    "test-watch": "nodemon --exec 'npm test'",
    "precommit": "lint-staged",
    "eslint": "node_modules/.bin/eslint src/",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Honchar007/Chillax.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Honchar007/Chillax/issues"
  },
  "homepage": "https://github.com/Honchar007/Chillax#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^6.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "mongoose": "^5.12.3",
    "next-auth": "^3.14.8",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "websocket": "^1.0.34"
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "node_modules/.bin/eslint --max-warnings=0",
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "expect": "^27.0.1",
    "mocha": "^8.4.0",
    "supertest": "^6.1.3"
  }
}
